{
  "name": "ui-shared-layout",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ui/shared-layout/src",
  "projectType": "library",
  "tags": ["type:ui", "scope:shared"],
  "implicitDependencies": [
    "data-access-auth",
    "data-access-user-settings",
    "data-access-websocket",
    "util-reading-aid"
  ],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/ui/shared-layout",
        "main": "libs/ui/shared-layout/src/index.ts",
        "tsConfig": "libs/ui/shared-layout/tsconfig.lib.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/ui/shared-layout/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/ui/shared-layout/jest.config.ts"
      }
    }
  }
}